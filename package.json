{
  "name": "goyais",
  "private": true,
  "version": "0.1.0",
  "packageManager": "pnpm@10.11.0",
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "dev:runtime": "pnpm --filter @goyais/python-agent dev",
    "dev:desktop": "pnpm --filter @goyais/desktop-tauri tauri dev",
    "dev:sync": "pnpm --filter @goyais/sync-server dev",
    "dev:hub": "cd server/hub-server-go && go run cmd/hub/main.go",
    "test": "turbo run test",
    "lint": "turbo run lint",
    "typecheck": "turbo run typecheck",
    "protocol:generate": "pnpm --filter @goyais/protocol generate"
  },
  "devDependencies": {
    "@types/node": "^22.13.10",
    "eslint": "^9.22.0",
    "prettier": "^3.5.3",
    "turbo": "^2.4.4",
    "typescript": "^5.8.2",
    "vitest": "^3.0.8"
  }
}
