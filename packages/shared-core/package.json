  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown src/index.ts --format esm,cjs --dts",
    "clean": "rm -rf dist"
  },
  "devDependencies": {
    "tsdown": "catalog:frontend",
    "typescript": "catalog:dev"
  }
}
